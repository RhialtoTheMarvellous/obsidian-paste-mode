{
  "name": "obsidian-paste-to-current-indentation",
  "version": "2.1.1",
  "description": "This plugin allows pasting and marking text as block-quotes at any level of indentation.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "rm -rf test/empty_vault && yarn run build && mkdir -p test/empty_vault/.obsidian/plugins/sample-plugin && cp manifest.json styles.css main.js test/empty_vault/.obsidian/plugins/sample-plugin && npx wdio run wdio.conf.js"
  },
  "keywords": [],
  "repository": "https://github.com/publicus/obsidian-paste-to-current-indentation",
  "author": "Jacob Levernier",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@wdio/cli": "^7.16.15",
    "@wdio/local-runner": "^7.16.15",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/selenium-standalone-service": "^7.16.14",
    "@wdio/spec-reporter": "^7.16.14",
    "builtin-modules": "^3.2.0",
    "chai": "^4.3.6",
    "chromedriver": "^98.0.1",
    "esbuild": "0.13.12",
    "jest": "^27.0.6",
    "mocha": "^9.0.2",
    "obsidian": "^0.13.30",
    "rollup": "^2.32.1",
    "spectron": "^15.0.0",
    "ts-jest": "^27.0.3",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "wdio-chromedriver-service": "^7.2.8"
  },
  "dependencies": {}
}
